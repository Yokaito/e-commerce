{
  "name": "@quixer/ui",
  "version": "0.0.9",
  "description": "A lightweight, framework agnostic component library for React",
  "author": "Guilherme Fontes <gui.fontes.amorim@gmail.com> (@guifontes)",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Yokaito/quixer",
    "directory": "packages/ui"
  },
  "browserslist": "supports es6-module and not dead and last 2 version",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint src/**/*.{ts,tsx}",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "size-limit": [
    {
      "path": "dist/esm/index.js",
      "limit": "40 KB"
    }
  ],
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@quixer/eslint-config": "workspace:*",
    "@quixer/shared": "workspace:*",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@size-limit/esbuild-why": "^8.2.6",
    "@size-limit/preset-small-lib": "^8.2.6",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "classnames": "^2.3.2",
    "eslint": "^8.44.0",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.25",
    "react": "^18.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^8.2.6",
    "typescript": "^5.1.6"
  },
  "volta": {
    "node": "16.18.0",
    "yarn": "1.19.1"
  }
}
