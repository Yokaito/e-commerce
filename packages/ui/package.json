{
  "name": "@quixer/ui",
  "version": "0.0.38",
  "description": "A lightweight, framework agnostic component library for React",
  "author": "Guilherme Fontes <gui.fontes.amorim@gmail.com> (@guifontes)",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Yokaito/quixer",
    "directory": "packages/ui"
  },
  "browserslist": "supports es6-module and not dead and last 2 version",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint src/**/*.{ts,tsx}",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "test": "jest --no-cache",
    "test:coverage": "jest --coverage",
    "storybook": "storybook dev -p 6006 -s public",
    "build-storybook": "storybook build -s public",
    "serve-storybook": "serve storybook-static"
  },
  "size-limit": [
    {
      "path": "dist/esm/index.js",
      "limit": "40 KB"
    }
  ],
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.22.7",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@quixer/eslint-config": "workspace:*",
    "@quixer/shared": "workspace:*",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@size-limit/esbuild-why": "^8.2.6",
    "@size-limit/preset-small-lib": "^8.2.6",
    "@storybook/addon-essentials": "7.0.26",
    "@storybook/addon-interactions": "7.0.26",
    "@storybook/addon-links": "7.0.26",
    "@storybook/addon-styling": "^1.3.2",
    "@storybook/blocks": "7.0.26",
    "@storybook/react": "7.0.26",
    "@storybook/react-webpack5": "7.0.26",
    "@storybook/testing-library": "0.0.14-next.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "babel-jest": "^29.6.1",
    "classnames": "^2.3.2",
    "eslint": "^8.44.0",
    "eslint-plugin-storybook": "^0.6.12",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.25",
    "prop-types": "15.8.1",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.63.6",
    "serve": "^14.2.0",
    "size-limit": "^8.2.6",
    "storybook": "7.0.26",
    "typescript": "^5.1.6"
  },
  "volta": {
    "node": "16.18.0",
    "yarn": "1.19.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "dependencies": {
    "modern-normalize": "^2.0.0"
  }
}
